    Method UpdateTitleColor(properties[])
        titleForeColor  
        titleBackColor  
    Begin
        Select Case GetCellBackColor(properties)
            Case GRID_BOX_CELL_BACK_COLOR_LIGHT_GRAY
                titleForeColor = TERMINAL_SETTING_FORE_COLOR_GRID_BOX_TITLE_LIGHT_GRAY
                titleBackColor = TERMINAL_SETTING_BACK_COLOR_GRID_BOX_TITLE_LIGHT_GRAY
            Case GRID_BOX_CELL_BACK_COLOR_BLUE
                titleForeColor = TERMINAL_SETTING_FORE_COLOR_GRID_BOX_TITLE_BLUE
                titleBackColor = TERMINAL_SETTING_BACK_COLOR_GRID_BOX_TITLE_BLUE
            Case GRID_BOX_CELL_BACK_COLOR_RED
                titleForeColor = TERMINAL_SETTING_FORE_COLOR_GRID_BOX_TITLE_RED
                titleBackColor = TERMINAL_SETTING_BACK_COLOR_GRID_BOX_TITLE_RED
            Case GRID_BOX_CELL_BACK_COLOR_GREEN
                titleForeColor = TERMINAL_SETTING_FORE_COLOR_GRID_BOX_TITLE_GREEN
                titleBackColor = TERMINAL_SETTING_BACK_COLOR_GRID_BOX_TITLE_GREEN
            Case GRID_BOX_CELL_BACK_COLOR_ORANGE
                titleForeColor = TERMINAL_SETTING_FORE_COLOR_GRID_BOX_TITLE_ORANGE
                titleBackColor = TERMINAL_SETTING_BACK_COLOR_GRID_BOX_TITLE_ORANGE
            Case GRID_BOX_CELL_BACK_COLOR_DARK_GRAY
                titleForeColor = TERMINAL_SETTING_FORE_COLOR_GRID_BOX_TITLE_DARK_GRAY
                titleBackColor = TERMINAL_SETTING_BACK_COLOR_GRID_BOX_TITLE_DARK_GRAY
            Case Else
                Return(0)
        End Select

        ColHeader(properties, true, titleForeColor, titleBackColor)
        RowHeader(properties, false, titleForeColor, titleBackColor)

    End Method

