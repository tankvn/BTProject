Package BtTopMenuWindow
    _isInitialized = false
    
    Method New()
    Begin
        BtTopMenuWindow_res:InitializeComponent()
        
        BtTopMenuWindow_FocusTransitionUtility:SetControl("BtTopMenuWindow_BtMenuItem_Storage")
        BtTopMenuWindow_FocusTransitionUtility:SetControl("BtTopMenuWindow_BtMenuItem_Retrieval")
        BtTopMenuWindow_FocusTransitionUtility:SetControl("BtTopMenuWindow_BtMenuItem_Configuration")
        BtTopMenuWindow_FocusTransitionUtility:SetControl("BtTopMenuWindow_BtButton_Exit")
    End Method
    
    Method Initialize()
    Begin
        BtTopMenuWindow_res:InitializeChildren()
        
        BtTopMenuWindow_BtMenuItem_Storage:Invalidate()
        BtTopMenuWindow_BtMenuItem_Retrieval:Invalidate()
        BtTopMenuWindow_BtMenuItem_Configuration:Invalidate()
        BtTopMenuWindow_BtButton_Exit:Invalidate()
        _isInitialized = true
    End Method
    
    Method Finalized()
    Begin
        BtTopMenuWindow_res:Dispose()
    End Method
    
    Method BtMenuItem_Storage_DataInput(sender, result)
    Begin
        BtTopMenuWindowForm:ShowBtJob_Storage()
        Return (result)
    End Method
    
    Method BtMenuItem_Retrieval_DataInput(sender, result)
    Begin
        BtTopMenuWindowForm:ShowBtJob_Retrieval()
        Return (result)
    End Method
    
    Method BtMenuItem_Configuration_DataInput(sender, result)
    Begin
        BtTopMenuWindowForm:ShowConfiguration()
        Return (result)
    End Method
    
    Method BtButton_Exit_DataInput(sender, result)
    Begin
        If MessageUtility:ShowMessage(MESSAGE_QUESTION_APPLICATION_EXIT, "", MESSAGE_BOX_BUTTONS_YES_NO, MESSAGE_BOX_ICON_QUESTION, MESSAGE_BOX_DEFAULT_BUTTON_BUTTON1) == DIALOG_RESULT_YES Then
            BtTopMenuWindowForm:ExitTerminal()
        End If
        
        Return (result)
    End Method
End Package
