        Return(InterfaceCommon:Call(toolName, Names, "PreprocessEnterData", args, ICommons))
    End Method

    Method ReadScanData(toolName)
        args[1]
    Begin
        Return(InterfaceCommon:Call(toolName, Names, "ReadScanData", args, ICommons))
    End Method

End Package

